MAINS = three-words divisors primality looking harmonic-one harmonic-two position ith aproximation parentheses palindrome perfect-numbers

all: $(MAINS)

# Construye los ejecutables
three-words: ej01-three-words.cc
	g++ -Wall -std=c++14 -o $@ $^
divisors: ej02-divisors-in-order.cc
	g++ -Wall -std=c++14 -o $@ $^
primality: ej03-primality.cc
	g++ -Wall -std=c++14 -o $@ $^
looking: ej04-looking-for-an-a.cc
	g++ -Wall -std=c++14 -o $@ $^
harmonic-one: ej05-harmonic-numbers.cc
	g++ -Wall -std=c++14 -o $@ $^
harmonic-two: ej06-harmonic-numbers-two.cc
	g++ -Wall -std=c++14 -o $@ $^
position: ej07-first-position.cc
	g++ -Wall -std=c++14 -o $@ $^
ith: ej08-ith.cc
	g++ -Wall -std=c++14 -o $@ $^
aproximation: ej09-approximation-of-e.cc
	g++ -Wall -std=c++14 -o $@ $^
parentheses: ej10-parentheses.cc
	g++ -Wall -std=c++14 -o $@ $^
palindrome: ej11-is-palindrome.cc
	g++ -Wall -std=c++14 -o $@ $^
perfect-numbers: ej12-perfect-numbers
	g++ -Wall -std=c++14 -o $@ $^


clean:
	rm -f three-words divisors primality looking harmonic-one harmonic-two position ith aproximation parentheses palindrome perfect-numbers